<mujoco model="fredo1">
    <compiler angle="degree"/>  <!-- use radians to define angles -->
    <default>
        <geom type="mesh" xyaxes="1 0 0 0 0 1"/>
    </default>
    
    <asset>
        <mesh name="base" file="all_files/a_base.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo1a" file="all_files/a_servo_1.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo1b" file="all_files/a_servo_2.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo1c" file="all_files/a_servo_3.STL" scale="0.001 0.001 0.001"/>

        <mesh name="servo1d" file="all_files/a_servo_4.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servoarm1" file="all_files/b_servoarm.STL" scale="0.001 0.001 0.001"/>

        <mesh name="arm1" file="all_files/b_firstarm.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo2a" file="all_files/b_servo_1.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo2b" file="all_files/b_servo_2.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo2c" file="all_files/b_servo_3.STL" scale="0.001 0.001 0.001"/>
        <mesh name="rod1" file="all_files/b_rod.STL" scale="0.001 0.001 0.001"/>

        <mesh name="servo2d" file="all_files/b_servo_4.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servoarm2" file="all_files/c_servoarm.STL" scale="0.001 0.001 0.001"/>

        <mesh name="arm2" file="all_files/c_secondarm.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo3a" file="all_files/c_servo_1.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo3b" file="all_files/c_servo_2.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servo3c" file="all_files/c_servo_3.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servoplate" file="all_files/c_servoplate.STL" scale="0.001 0.001 0.001"/>
        <mesh name="rod2" file="all_files/c_rod.STL" scale="0.001 0.001 0.001"/>

        <mesh name="servo3d" file="all_files/c_servo_4.STL" scale="0.001 0.001 0.001"/>
        <mesh name="servoarm3" file="all_files/d_servoarm.STL" scale="0.001 0.001 0.001"/>
        
        <mesh name="arm3" file="all_files/d_thirdarm.STL" scale="0.001 0.001 0.001"/>
        <mesh name="tippoint" file="all_files/d_tippoint.STL" scale="0.001 0.001 0.001"/>
    </asset>
    
    <worldbody>

        <geom name="origin_marker" type="sphere" size="0.01" rgba="1 0 0 1" pos="0 0 0"/>

        <geom name="x_axis" type="cylinder" fromto="0 0 0 0.05 0 0" size="0.002" rgba="1 0 0 1"/>
        <geom name="y_axis" type="cylinder" fromto="0 0 0 0 0.05 0" size="0.002" rgba="0 1 0 1"/>
        <geom name="z_axis" type="cylinder" fromto="0 0 0 0 0 0.05" size="0.002" rgba="0 0 1 1"/>
            
    	<body name="base">
            <geom mesh="base"/>                            
            <body name="servo1a" pos="0 0 0">
                <geom mesh="servo1a"/>   
	        </body>
            <body name="servo1b" pos="0 0 0">
                <geom mesh="servo1b"/>   
	        </body>
            <body name="servo1c" pos="0 0 0">
                <geom mesh="servo1c"/>   
	        </body>
            
            <!-- new joint -->
            <body name="servo1d" pos="0 0 0">
                <geom mesh="servo1d"/>   
                <body name="servoarm1" pos="0 0 0">
                    <geom mesh="servoarm1"/>   
                    <joint name="joint_1" type="hinge" pos="0.01916 -0.05981 0.06072" axis="0 0 1" limited="true" range="-80 180"/> 
                    <body name="arm1" pos="0 0 0">
                        <geom mesh="arm1"/>   
                    </body>
                    <body name="servo2a" pos="0 0 0">
                        <geom mesh="servo2a"/>   
                    </body>
                    <body name="servo2b" pos="0 0 0">
                        <geom mesh="servo2b"/>   
                    </body>
                    <body name="servo2c" pos="0 0 0">
                        <geom mesh="servo2c"/>   
                    </body>
                    <body name="rod1" pos="0 0 0">
                        <geom mesh="rod1"/>   
                    </body>

                    <!-- new joint -->
                    <body name="servo2d" pos="0 0 0">
                        <geom mesh="servo2d"/>   
                        <body name="servoarm2" pos="0 0 0">
                            <geom mesh="servoarm2"/>   
                            <joint name="joint_2" type="hinge" pos="-0.01074 -0.07031 0.09422" axis="1 0 0" limited="true" range="-85 90"/> 
                            <body name="arm2" pos="0 0 0">
                                <geom mesh="arm2"/>   
                            </body>
                            <body name="servo3a" pos="0 0 0">
                                <geom mesh="servo3a"/>   
                            </body>
                            <body name="servo3b" pos="0 0 0">
                                <geom mesh="servo3b"/>   
                            </body>
                            <body name="servo3c" pos="0 0 0">
                                <geom mesh="servo3c"/>   
                            </body>
                            <body name="servoplate" pos="0 0 0">
                                <geom mesh="servoplate"/>   
                            </body>
                            <body name="rod2" pos="0 0 0">
                                <geom mesh="rod2"/>   
                            </body>

                            <!-- new joint -->
                            <body name="servo3d" pos="0 0 0">
                                <geom mesh="servo3d"/>   
                                <body name="servoarm3" pos="0 0 0">
                                    <geom mesh="servoarm3"/>   
                                    <joint name="joint_3" type="hinge" pos="-0.01214 -0.08073 0.19022" axis="1 0 0" limited="true" range="-85 90"/> 
                                    <body name="arm3" pos="0 0 0">
                                        <geom mesh="arm3"/>   
                                    </body>
                                    <body name="tippoint" pos="0 0 0">
                                        <geom mesh="tippoint"/>   
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
	        </body>
	    </body>
    	
    </worldbody>
    
    <actuator>
        <position name="joint_1" ctrllimited="true" kp="1" kv="0.15" joint="joint_1" ctrlrange="-1.57 3.14"/>
        <position name="joint_2" ctrllimited="true" kp="1" kv="0.15" joint="joint_2" ctrlrange="-1.57 1.57"/>
        <position name="joint_3" ctrllimited="true" kp="1" kv="0.15" joint="joint_3" ctrlrange="-1.57 1.57"/>
        
    </actuator>

    <contact>
        <exclude body1="servoarm1" body2="servo1d"/>
        <exclude body1="servoarm1" body2="servo1c"/>
        <exclude body1="servoarm1" body2="servo1b"/>
        <exclude body1="servoarm1" body2="servo1a"/> 
        <exclude body1="servoarm1" body2="base"/>         
    </contact>
</mujoco>