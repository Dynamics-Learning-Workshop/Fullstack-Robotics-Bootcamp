        <!-- Ground plane -->

        
        <!-- Coordinate axes as cylinders -->
        <geom name="x_axis" type="cylinder" fromto="0 0 0 0.1 0 0" size="0.002" rgba="1 0 0 1"/>
        <geom name="y_axis" type="cylinder" fromto="0 0 0 0 0.1 0" size="0.002" rgba="0 1 0 1"/>
        <geom name="z_axis" type="cylinder" fromto="0 0 0 0 0 0.1" size="0.002" rgba="0 0 1 1"/>
        
