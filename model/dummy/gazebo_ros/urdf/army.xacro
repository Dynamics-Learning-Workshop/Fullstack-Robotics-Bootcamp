<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="army">

  <!-- Load materials -->
  <xacro:include filename="$(find army_description)/urdf/materials.xacro"/>

  <!-- Load links -->
  <xacro:include filename="$(find army_description)/urdf/links/base_link.xacro"/>
  <xacro:include filename="$(find army_description)/urdf/links/link1.xacro"/>
  <xacro:include filename="$(find army_description)/urdf/links/link2.xacro"/>
  <!-- <xacro:include filename="$(find army_description)/urdf/links/end_effector.xacro"/> -->

  <!-- Load joints -->
  <xacro:include filename="$(find army_description)/urdf/joints/joint0.xacro"/>
  <xacro:include filename="$(find army_description)/urdf/joints/joint1.xacro"/>
  <xacro:include filename="$(find army_description)/urdf/joints/joint2.xacro"/>
  <!-- <xacro:include filename="$(find army_description)/urdf/joints/joint3.xacro"/> -->

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <!-- <robotNamespace>/army</robotNamespace> -->
    </plugin>
  </gazebo>

</robot>
